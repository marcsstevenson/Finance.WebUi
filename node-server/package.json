{
  "name": "ng2-startingline",
  "version": "1.0.0",
  "dependencies": {
    "express": "4.13.4"
  },
  "scripts": {
    "build:nodeserver": "ng build && cp node-server\/* dist",
    "build:nodeserver-prod": "ng build -prod && cp node-server\/* dist",
    "serve-build": "npm run build:nodeserver && cd dist && node index.js",
    "serve-build-prod": "npm run build:nodeserver-prod && cd dist && node index.js",

    "start": "ng serve",
    "postinstall": "typings install",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  }
}